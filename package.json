{
  "name": "alphacodehub",
  "displayName": "Alpha Theme",
  "description": "A VSCode theme inspired by Vercel Theme and One Dark Pro",
  "publisher": "Alpha",
  "version": "1.3.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      
      {
        "label": "Alpha Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Alpha-Dark-color-theme.json"
      },
      {
        "label": "Alpha Light",
        "uiTheme": "vs-light",
        "path": "./themes/Alpha-Light-color-theme.json"
      }
    ]
  },
  "configuration": {
    "title": "Alpha Theme",
    "properties": {
      "one-hunter.editorTheme": {
        "type": "string",
        "default": "classic",
        "markdownDescription": "%description.one-hunter.editorTheme%",
        "enum": [
          "Alpha Dark",
          "Alpha light"
      
        ],
        "enumDescriptions": [
          "Alpha Dark",
          "Alpha Light"
        ],
        "description": "Select the theme variant"
      }
    }
  },
  "keywords": [
    "Alpha light",
    "Alpha Dark"
    
  ],
  "icon": "icon.png",
  "homepage": "https://github.com/Railly/one-hunter-vscode/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/Railly/one-hunter-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Railly/one-hunter-vscode.git"
  },
  
 
}